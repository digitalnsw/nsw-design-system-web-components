{
  "name": "@digitalnsw/nsw-accordion",
  "wcfactory": {
    "className": "NswAccordion",
    "customElementClass": "LitElement",
    "elementName": "nsw-accordion",
    "generator-wcfactory-version": "0.12.4",
    "useHAX": false,
    "useSass": true,
    "files": {
      "css": "src/nsw-accordion.css",
      "scss": "src/nsw-accordion.scss",
      "html": "src/nsw-accordion.html",
      "js": "src/nsw-accordion.js",
      "properties": "src/nsw-accordion-properties.json",
      "hax": "src/nsw-accordion-hax.json"
    },
    "iconset": {},
    "sharedHaxProps": [],
    "sharedProps": [],
    "sharedStyles": []
  },
  "version": "0.0.4",
  "description": "An accordion is a series of expandable vertical panels, designed to save space on a page by hiding content and revealing it as required.",
  "repository": {
    "type": "git",
    "url": "git@github.com:rod-higgins/nsw-design-system-wc.git"
  },
  "main": "nsw-accordion.js",
  "module": "nsw-accordion.js",
  "umd": "nsw-accordion.umd.js",
  "scripts": {
    "test": "web-test-runner \"test/**/*.test.js\" --node-resolve",
    "test:watch": "web-test-runner \"test/**/*.test.js\" --node-resolve --watch",
    "test:browsers": "web-test-runner \"test/**/*.test.js\" --node-resolve --playwright --browsers chromium firefox webkit",
    "start": "yarn run dev",
    "build": "gulp --gulpfile=gulpfile.cjs && node --experimental-json-modules ../../node_modules/.bin/rollup --config && prettier --ignore-path ../../.prettierignore --write '**/*.{js,json}' && wca analyze \"**/*.js\" --outFile custom-elements.json",
    "dev": "concurrently --kill-others \"yarn run watch\" \"yarn run serve\"",
    "watch": "gulp dev --gulpfile=gulpfile.cjs",
    "serve": "es-dev-server -c ../../es-dev-server.config.js",
    "lighthouse": "gulp lighthouse --gulpfile=gulpfile.cjs"
  },
  "author": {
    "name": "Rod Higgins"
  },
  "license": "MIT",
